{
    "name": "accesside",
    "version": "1.0.0",
    "description": "A web based IDE to aid Blind and Visually impared developers write code",
    "main": "server.js",
    "scripts": {
        "test": "run-s build:typescript build:next",
        "dev:node": "nodemon -r esm server/dist/server.js",
        "dev:typescript": "gulp dev",
        "build:next": "next build",
        "build:typescript": "gulp",
        "build:server": "node -r esm server/dist/server.js",
        "build:lint": "lint-fix",
        "dev": "run-p dev:*",
        "start": "run-s build:*",
        "lint": "eslint \"./server/src/**/*.ts\" \"./pages/*.tsx\" \"./components/*.tsx\"",
        "lint-fix": "eslint \"./server/src/**/*.ts\" \"./pages/*.tsx\" \"./components/*.tsx\" --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navidmx/AccessIDE.git"
    },
    "author": "Navid Mamoon and Garrett Burroughs",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/navidmx/AccessIDE/issues"
    },
    "homepage": "https://github.com/navidmx/AccessIDE#readme",
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.20",
        "@fortawesome/free-solid-svg-icons": "^5.10.0",
        "@fortawesome/react-fontawesome": "^0.1.4",
        "@google-cloud/speech": "^3.2.4",
        "@types/body-parser": "^1.17.0",
        "@typescript-eslint/eslint-plugin": "^2.3.1",
        "@typescript-eslint/parser": "^2.3.1",
        "bootstrap": "^4.3.1",
        "brace": "^0.11.1",
        "child-process": "^1.0.2",
        "eslint": "^6.5.0",
        "eslint-config-prettier": "^6.3.0",
        "eslint-plugin-prettier": "^3.1.1",
        "eslint-plugin-react": "^7.14.3",
        "express": "^4.17.1",
        "ffmpeg": "0.0.4",
        "fluent-ffmpeg": "^2.1.2",
        "gulp-eslint": "^6.0.0",
        "gulp-typescript": "^5.0.1",
        "jquery": "^3.5.0",
        "next": "^9.0.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.18.2",
        "react": "^16.8.6",
        "react-ace": "^7.0.2",
        "react-bootstrap": "^1.0.0-beta.10",
        "react-dom": "^16.8.6",
        "react-dropdown": "^1.6.4",
        "react-mic": "^12.4.1",
        "request": "^2.88.0",
        "ts.data.json": "^0.2.0"
    },
    "devDependencies": {
        "@types/jquery": "^3.3.30",
        "@types/node-fetch": "^2.5.0",
        "@types/react": "^16.8.23",
        "@types/react-dom": "^16.8.5",
        "esm": "^3.2.25",
        "gulp": "^4.0.2",
        "gulp-rimraf": "^1.0.0",
        "nodemon": "^1.19.1",
        "typescript": "^3.5.3"
    }
}
